{
    "name": "alpine-ts",
    "version": "0.3.1",
    "description": "Alpine - the Apache Log Parser in Typescript",
    "main": "src/index.js",
    "types": "src/index.d.ts",
    "directories": {
        "test": "test"
    },
    "dependencies": {
        "byline": "^5.0.0",
        "through2": "^4.0.2"
    },
    "devDependencies": {
        "@types/byline": "^4.2.32",
        "@types/mocha": "^8.2.0",
        "@types/through2": "^2.0.36",
        "coveralls": "^3.1.0",
        "mocha": "^8.3.0",
        "mocha-lcov-reporter": "^1.3.0",
        "nyc": "^15.1.0",
        "rimraf": "^3.0.2",
        "ts-node": "^9.1.1",
        "typescript": "^4.1.5"
    },
    "scripts": {
        "clean": "rimraf {src,test}/{*.map,*.d.ts,*.js}",
        "test": "nyc --extension=.ts --reporter=html --reporter=lcov --reporter=text mocha --timeout 10000 --exit --require ts-node/register test/*.test.ts",
        "coverage": "nyc report --reporter=text-lcov | coveralls",
        "build": "tsc -p .",
        "prepare": "npm run build"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/UrielCh/node-alpine-ts.git"
    },
    "keywords": [
        "apache",
        "mod_log",
        "log",
        "parser",
        "combined",
        "clf"
    ],
    "author": "uriel chemouni <uchemouni@gmail.com>",
    "contributors": [{
            "name": "Bj√∏rn Hell Larsen",
            "email": "blarsen@nonews.net"
        },
        {
            "name": "uriel chemouni",
            "email": "uchemouni@gmail.com",
            "url": "https://urielch.github.io/urielch/"
        }
    ],
    "license": "Apache License 2.",
    "bugs": {
        "url": "https://github.com/UrielCh/node-alpine-ts/issues"
    },
    "homepage": "https://github.com/UrielCh/node-alpine-ts",
    "files": [
        "src/*.js",
        "src/*.d.ts"
    ]
}